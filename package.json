{
  "name": "gostack-gobarber-v2-api",
  "version": "1.0.0",
  "repository": "git@github.com:pgomesdev/gostack-gobarber-v2-api.git",
  "author": "pgomesdev <pgomesdev@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "ts-node-dev --transpileOnly --ignore-watch node_modules src/server.ts",
    "build": "tsc"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-typescript": "^2.2.1",
    "eslint-plugin-import": "^2.21.2",
    "husky": "^4.2.5",
    "ts-node-dev": "^1.0.0-pre.61",
    "typescript": "^4.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && node_modules/.bin/git-cz --hook || true"
    }
  }
}
